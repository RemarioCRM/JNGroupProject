<apex:page applyBodyTag="false" controller="MultiProductCalculatorDocController" renderAs="PDF">
    <head>
        <style type="text/css" media="print">
            @page {
            @top-center {
            content: element(header);
            }
            @bottom-left {
            content: element(footer);
            }
            }
            div.header {
            padding: 10px;
            position: running(header);
            }
            div.footer {
            display: block;
            padding: 5px;
            margin-bottom:100px;
            position: running(footer);
            }
            
            .pagenumber:before {
            content: counter(page);
            }
            .pagecount:before {
            content: counter(pages);
            }
        </style>
    </head>
    <apex:form >
        <!--<apex:outputText value="{0,date,dd-MM-yy}">
<apex:param value="{!TODAY()}" />                                         
</apex:outputText>-->  
        <div class="footer">
            <div style="text-align:left;font-size:15px;">{!$Label.ApplicationFormText} &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                Multi_Product_Calculator   &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                &nbsp;&nbsp;&nbsp;<span class="pagenumber"/> of <span class="pagecount"/>
            </div>
            
        </div>
        <div class="content">
            <apex:pageBlock rendered="{!if(ProductDetails.size>0, true, false)}">
                <apex:image url="{!URLFOR($Resource.MultiProductCalculatorHeader)}" style="width:100%; height:130px;"/>
                <h5 style="font-size:13px; color:black;background-color: #92d5f0; height:17px; padding-top:15px;">&nbsp;MULTI-PRODUCT CALCULATOR (INSTALLMENT &amp; REVOLVING)</h5>
                <h5 style="width:98%;font-size:Small; color:#00a1e0;border-bottom: 2px solid rgb(149, 149, 149);">&nbsp;PRODUCT SELECTION &amp; APPLICANT DETAILS</h5>
                <div style="font-size:Small;">
                    <div style="width:33%;display:inline-block;">
                        <div style="border: 1px solid white;">Select the applicable product combination*</div>
                    </div>
                    <div style="width:30%;display:inline-block;">
                        <div style="border: 1px solid rgb(191, 191, 191);height:30px;text-align:center;">{!productName}</div>
                    </div>
                    <div style="width:20%;display:inline-block;">
                        <div style="border: 1px solid white;">Number of Applicant(s)*</div>
                    </div>
                    <div style="width:15%;display:inline-block;">
                        <div style="border: 1px solid rgb(191, 191, 191);height:20px;text-align:center;vertical-align:top;">{!ApplicantCount} Applicant</div>
                    </div>
                </div>
                <div class="table-top" style="font-size:10px;">
                <table class="content-table">
                    <tr>
                       
                        <td style="width:2%;"></td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);background-color:rgb(242, 242, 242);">APPLICANT'S LAST NAME</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;background-color:rgb(242, 242, 242);">APPLICANT'S FIRST NAME</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;background-color:rgb(242, 242, 242);">DATE OF BIRTH*(MM/DD/YYYY)</td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;background-color:rgb(242, 242, 242);">GROSS MONTHLY INCOME*</td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;background-color:rgb(242, 242, 242);">EXISTING MONTHLY CREDIT PAYMENTS</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;background-color:rgb(242, 242, 242);">JN EMPLOYEE* (check the box if YES)</td>
                    
                    </tr>
                    </table>
                  
                    <apex:outputPanel rendered="{!if(ApplicantCount=='One (1)' || ApplicantCount=='Two (2)'|| ApplicantCount=='Three (3)',true,false)}">
                        <table style="width:100%" class="content-table">
                        <tr>
                       <td  style="width:2%;border: 1px solid rgb(191, 191, 191);">1</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!Applicant1last}</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!Applicant1first} </td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;"><apex:outputText value="{0,date,MM-dd-yyyy}">
                            <apex:param value="{!ProductDetails[0].Applicant_1_Date_of_Birth__c}" />                                         
                            </apex:outputText></td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_1_Gross_Monthly_Income__c}</td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_1_Existing_Monthly_Payment__c}</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_1_Jn_Employee__c}</td>
                        </tr>
                        </table>
                    </apex:outputPanel>
                <apex:outputPanel rendered="{!if(ApplicantCount=='Two (2)' || ApplicantCount=='Three (3)',true,false)}">
                        <table style="width:100%" class="content-table">
                        <tr>
                       <td  style="width:2%;border: 1px solid rgb(191, 191, 191);">2</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!Applicant2last}</td>
                        <td  style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!Applicant2first}</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;"><apex:outputText value="{0,date,MM-dd-yyyy}">
                            <apex:param value="{!ProductDetails[0].Applicant_2_Date_of_Birth__c}" />                                         
                            </apex:outputText></td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_2_Gross_Monthly_income__c}</td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_2_existing_monthly_credit__c}</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_2_JN_Employee__c}</td>
                        </tr>
                        </table>
                    </apex:outputPanel>
                   
                        <apex:outputPanel rendered="{!if(ApplicantCount=='Three (3)',true,false)}">
                        <table style="width:100%" class="content-table">
                        <tr>
                       <td  style="width:2%;border: 1px solid rgb(191, 191, 191);">3</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!Applicant3last}</td>
                        <td  style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!Applicant3first}</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;"><apex:outputText value="{0,date,MM-dd-yyyy}">
                            <apex:param value="{!ProductDetails[0].Applicant_3_Date_of_Birth__c}" />                                         
                            </apex:outputText></td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_3_Gross_Monthly_income__c}</td>
                        <td style="width:19%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_3_Existing_Monthly__c}</td>
                        <td style="width:15%;border: 1px solid rgb(191, 191, 191);border-left:1px;">{!ProductDetails[0].Applicant_3_Jn_Employee__c}</td>
                        </tr>
                        </table>
                    </apex:outputPanel>
                    
            </div>
                <apex:pageBlock rendered="{!isAutoFound}">
                    <h5 style="width:98%;font-size:Small; color:#00a1e0;border-bottom: 2px solid rgb(149, 149, 149);">&nbsp;PERSONAL AUTO LOAN DETAILS</h5>
                    <div style="margin-top:20px;margin-bottom:15px;font-size:small;">
                        <div style="width:20%;display:inline-block;">
                            <div style="border: 1px solid white;">Loan Purpose*</div>
                            <div style="border: 1px solid white;margin-top:10px;">Market Value of Vehicle*</div>
                        </div>
                        <div style="width:20%;display:inline-block;">
                            <div style="border: 1px solid rgb(191, 191, 191);height:20px;text-align:center;">{!ProductDetails[0].Auto_Loan_Purpose__c}</div>
                            <div style="border: 1px solid rgb(191, 191, 191);margin-top:10px;height:20px;text-align:center;">JMD ${!ProductDetails[0].Market_Value_of_Vehicle__c}</div>
                        </div>
                        <div style="width:20%;display:inline-block;margin-left:5px;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Auto_Loan_Purpose__c=='Equity in a Motor Vehicle',false,true)}">
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Auto_Loan_Purpose__c=='Purchase a Motor Vehicle',true,false)}">
                                    <div style="border: 1px solid white;">Purchase Price of Vehicle</div>
                                </apex:pageBlock>
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Auto_Loan_Purpose__c=='Refinance existing Auto Loan',true,false)}">
                                    <div style="border: 1px solid white;">Loan Balance of Vehicle</div>
                                </apex:pageBlock>
                            </apex:pageBlock>
                            <div style="border: 1px solid white;margin-top:10px;">Motor Vehicle Deposit*</div>
                        </div>
                        <div style="width:35%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Auto_Loan_Purpose__c=='Equity in a Motor Vehicle',false,true)}">
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Auto_Loan_Purpose__c=='Purchase a Motor Vehicle',true,false)}">
                                    <div style="width:55%;border: 1px solid rgb(191, 191, 191);height:20px;text-align:center;">JMD ${!ProductDetails[0].Purchase_Price_of_Vehicle__c}</div>
                                </apex:pageBlock>
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Auto_Loan_Purpose__c=='Refinance existing Auto Loan',true,false)}">
                                    <div style="width:55%;border: 1px solid rgb(191, 191, 191);height:20px;text-align:center;">JMD $</div>
                                </apex:pageBlock>
                                
                                <div>
                                    <div style="width:15%;display:inline-block;">
                                        <div style="border: 1px solid rgb(191, 191, 191);margin-top:10px;height:20px;text-align:center;">{!ProductDetails[0].Motor_Vehicle_Deposit_Percentage__c} %</div>
                                    </div>
                                    <div style="width:38%;display:inline-block;">
                                        <div style="border: 1px solid rgb(191, 191, 191);margin-top:10px;height:20px;text-align:center;">JMD ${!ProductDetails[0].Motor_Vehicle_Deposit__c}</div>
                                    </div>
                                    <div style="width:4%;display:inline-block;">
                                        <div style="margin-top:5px;color:red;">or</div>
                                    </div>
                                    <div style="width:38%;display:inline-block;">
                                        <div style="border: 1px solid rgb(191, 191, 191);margin-top:10px;height:20px;text-align:center;">JMD ${!ProductDetails[0].Motor_Vehicle_Deposit__c}</div>
                                    </div>
                                </div>
                                
                                
                            </apex:pageBlock>
                        </div>
                    </div>
                    <h5 style="border: 1px solid rgb(149, 149, 149);width:30%;background-color:rgb(242, 242, 242);font-family:Trebuchet MS; padding-top:12px; margin-left:1px;">&nbsp;AUTO LOAN FEATURES</h5>
                    <div style="border: 1px solid rgb(149, 149, 149);margin: 1px; margin-top:-20px;">
                        <div style="font-size:Small;font-style:italic;font-style:bold;margin-top:7px;"><u>JNGI SURE COVER PROGRAMME</u></div>
                        <div style="width:30%;display:inline-block;margin-top:5px;">
                            <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                <tr>
                                    <td>Interested in programme?*</td>
                                </tr>
                                <tr>
                                    <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Interested_in_programme__c}</td>
                                </tr>
                            </table>
                        </div>
                        <div style="width:40%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Interested_in_programme__c=='Yes',true,false)}">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td>Include 1st Year Premium in Loan Amount*</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Include_first_year_premium_in_loan_amt__c}</td>
                                    </tr>
                                </table>
                            </apex:pageBlock>
                        </div>
                        <div style="width:25%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Interested_in_programme__c=='Yes',true,false)}">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td>Monthly Premium*</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">JMD ${!ProductDetails[0].Monthly_Premium__c}</td>
                                    </tr>
                                </table>
                            </apex:pageBlock>
                        </div>
                        
                        <div style="font-size:Small;font-style:italic;font-style:bold;margin-top:7px;"><u>JN LIFE CREDITOR LIFE INSURANCE</u></div>
                        <div style="width:30%;display:inline-block;margin-top:5px;">
                            <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                <tr>
                                    <td>Interested in Creditor Life?*</td>
                                </tr>
                                <tr>
                                    <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Interested_in_Creditor_Life__c}</td>
                                </tr>
                            </table>
                        </div>
                        <div style="width:40%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Interested_in_Creditor_Life__c=='Yes',true,false)}">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td>Coverage Type*</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Coverage_Type__c}</td>
                                    </tr>
                                </table>
                            </apex:pageBlock>
                        </div>
                        <div style="width:25%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Interested_in_Creditor_Life__c=='Yes',true,false)}">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td>Include in Loan Amount*</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Include_in_Loan_Amount_jnlife__c}</td>
                                    </tr>
                                </table>
                            </apex:pageBlock>
                        </div>
                        <div style="font-size:Small;font-style:italic;font-style:bold;margin-top:7px;"><u>PROCESSING FEE</u></div>
                        <div style="width:30%;display:inline-block;margin-top:5px;">
                            <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                <tr>
                                    <td>Waive Processing Fees?*</td>
                                </tr>
                                <tr>
                                    <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Waive_Process_Fee__c}</td>
                                </tr>
                            </table>
                        </div>
                        <div style="width:40%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Waive_Process_Fee__c=='No',true,false)}">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td>Include in Loan Amount*</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Include_in_Loan_Amount_Processing_Fee__c}</td>
                                    </tr>
                                </table>
                            </apex:pageBlock>
                        </div>
                        <div style="width:25%;display:inline-block;">
                            <apex:pageBlock rendered="{!if(ProductDetails[0].Waive_Process_Fee__c=='No',true,false)}">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td colspan="2">Indicate applicable processing fees (%)*</td>
                                    </tr>
                                    <tr>
                                        <td style="width:50%;border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Indicate_applicable_process_fee_percent__c}%</td>
                                        <td style="width:50%;border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">per annum</td>
                                    </tr>
                                </table>
                            </apex:pageBlock>
                        </div>
                        <div style="font-size:Small;font-style:italic;font-style:bold;margin-top:7px;"><u>MORATORIUM</u></div>
                        <div>
                            <div style="width:35%;display:inline-block;margin-top:5px;">
                                <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                    <tr>
                                        <td>Include a moratorium of loan repayments*</td>
                                    </tr>
                                    <tr>
                                        <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Include_a_moratorium_of_Loan_Payment__c}</td>
                                    </tr>
                                </table>
                            </div>
                            <div style="width:40%;display:inline-block;">
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Include_a_moratorium_of_Loan_Payment__c=='Yes',true,false)}">
                                    <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                        <tr>
                                            <td>Indicate Type*</td>
                                        </tr>
                                        <tr>
                                            <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Indicate_Type__c}</td>
                                        </tr>
                                    </table>
                                </apex:pageBlock>
                            </div>
                            <div style="width:20%;display:inline-block;">
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Include_a_moratorium_of_Loan_Payment__c=='Yes',true,false)}">
                                    <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                        <tr>
                                            <td colspan="2">Indicate Term*</td>
                                        </tr>
                                        <tr>
                                            <td style="width:50%;border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Moratorium_Indicate_Term__c}</td>
                                            <td style="width:50%;border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">mnths</td>
                                        </tr>
                                    </table>
                                </apex:pageBlock>
                            </div>
                        </div>
                        <div>
                            <div style="width:40%;display:inline-block;">
                                <apex:pageBlock rendered="{!if(ProductDetails[0].Include_a_moratorium_of_Loan_Payment__c=='Yes',true,false)}">
                                    <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                        <tr>
                                            <td>Other post moratorium options*</td>
                                        </tr>
                                        <tr>
                                            <td style="border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;text-align:center;">{!ProductDetails[0].Othee_post_moratorium__c}</td>
                                        </tr>
                                    </table>
                                </apex:pageBlock>
                            </div>
                        </div>
                        <div style="font-size:Small;font-style:italic;font-style:bold;margin-top:7px;"><u>LOAN SAVINGS</u></div>
                        <div style="width:50%;display:inline-block;">
                            
                            <table style="font-size:Small;margin-left:10px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="5">
                                <tr>
                                    <td colspan="4">Proposed Savings</td>
                                </tr>
                                <tr>
                                    <td style="width:20;border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;">{!ProductDetails[0].Proposed_SavingsPercentage__c}</td>
                                    <td style="width:10;border: 1px solid rgb(191, 191, 191);Text-align:center;height:20px;">%</td>
                                    <td style="width:10;Text-align:center;height:20px;text-align:center;">OR</td>
                                    <td style="width:40;border: 1px solid rgb(191, 191, 191);Text-align:left;height:20px;">JMD ${!ProductDetails[0].Proposed_Savings__c}</td>
                                </tr>
                            </table>
                            
                        </div>
                    </div>
                    <h5 style="border: 1px solid rgb(149, 149, 149);width:30%;background-color:rgb(242, 242, 242);font-family:Trebuchet MS; padding-top:12px; margin-left:1px;">&nbsp;REQUESTED DETAILS</h5>
                    <div style="border: 1px solid rgb(149, 149, 149);margin: 1px; margin-top:-20px;">
                        <table style="font-size:Small;margin-left:16px;margin-right:10px;margin-top:5px;width:95%;" cellpadding="0" cellspacing="0">
                            <tr>
                                <td></td>
                                <td style="border: 1px solid rgb(191, 191, 191);background-color:rgb(242, 242, 242);" colspan="2">INTEREST RATE (%)*</td>
                                <td style="border: 1px solid rgb(191, 191, 191);background-color:rgb(242, 242, 242);border-left:1px;" colspan="4">LOAN TERM*</td>
                                <td style="border: 1px solid rgb(191, 191, 191);background-color:rgb(242, 242, 242);border-left:1px;" colspan="2">LOAN AMOUNT*</td>
                                
                            </tr>
                            <tr>
                                <td style="border: 1px solid rgb(191, 191, 191);background-color:rgb(242, 242, 242);border-right:1px;">Market</td>
                                <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;">{!ProductDetails[0].Interest_Rate__c}</td>
                                <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;">per annum</td>
                                <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;">{!ProductDetails[0].Years__c}</td>
                                <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;">yrs</td>
                                <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;">{!ProductDetails[0].Months__c}</td>
