<apex:page renderAs="PDF" controller="ApplicationForm">
    
    
    <apex:image url="{!URLFOR($Resource.ApplicationFormheader)}" style="width:100%; height:100px;"/>
    <apex:form >
        <script>
        function yr(obj){
var array = obj.split('.')
return array[1];
}
        </script>
        <apex:repeat value="{!lst}" var="app" id="repeatapp" rows="">
        <div style="margin-top:30px;">
        <h3 style="font-size:Small;width:50%;color:white;background-color: #00a1e0;font-family: sans-serif; height:35px; padding-top:12px; margin-left:1px;">
            &nbsp;PERSONAL &amp; FINANCIAL DETAILS - APPLICANT {!app.Appcount}
        </h3>
        <div style="border: 1px solid #00a1e0;margin: 1px; margin-top:-11px;">
            <h5 style="font-size:16px; color:black;background-color: #92d5f0; height:30px; padding-top:15px;"><b>
                &nbsp;PERSONAL DETAILS
                </b>
            </h5>
            <div>
                <h5 style="width:98%;font-size:Small; color:#00a1e0;margin-left:5px;border-bottom: 2px solid #00a1e0;"><b>
                    &nbsp;GENERAL DETAILS
                    </b>
                </h5>
                
                <div style="margin-top:5px;font-size:small;">
                    <table style="margin-left:16px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="0">
                        <tr>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-bottom:1px;">Title:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Last Name:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">First Name:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Middle Name:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Gender:</td>
                        </tr>
                        <tr style="margin-top:5px;">
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Salutation}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.FirstName}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.LastName}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.MiddleName}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.FinServ__Gender__pc}</td>
                        </tr>
                    </table>
                </div>
                <div style="margin-top:10px;font-size:small;">
                    <table style="margin-left:16px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="0">
                        <tr>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-bottom:1px;">Suffix:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Marital Status:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Date of Birth:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Place of Birth:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Mother's Maiden Name:</td>
                        </tr>
                        <tr>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Suffix__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.FinServ__MaritalStatus__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;"><apex:outputText value="{0,date,dd-MM-yy}">
        <apex:param value="{!app.ApplicantPersonAccount.Date_of_Birth__pc}" />                                         
    </apex:outputText></td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Place_of_Birth__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.FinServ__MotherMaidenName__pc}</td>
                        </tr> 
                    </table>
                </div>
                <div style="margin-top:10px;font-size:small;">
                    <table style="margin-left:16px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="0">
                        <tr>
                            <td colspan="4" style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-bottom:1px;">Number of Dependent(s):</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Country of Citizenship:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Country of Residence:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Highest level of education attained?</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Primary Type of Employment:</td>
                        </tr>
                        <tr>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-right:1px;Text-align:center;">{!app.ApplicantPersonAccount.FinServ__NumberOfDependents__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-left:1px;border-right:1px;Text-align:center;">Adult</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-left:1px;border-right:1px;Text-align:center;">{!app.ApplicantPersonAccount.FinServ__NumberOfDependents__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-left:1px;Text-align:center;">Childrem</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Country_of_Citizenship__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Country_of_Residency__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Highest_Level_of_Education_attained__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Primary_Employment_Type__pc}</td>
                        </tr> 
                    </table>
                </div>
            </div>
            <div>
                <h5 style="width:98%;font-size:Small; color:#00a1e0;margin-left:5px;border-bottom: 2px solid #00a1e0;"><b>
                    &nbsp;IDENTIFICATION DETAILS
                    </b>
                </h5>
                
                <div style="margin-top:5px;font-size:small;">
                    <table style="margin-left:16px;margin-right:10px;width:95%;" cellpadding="0" cellspacing="0">
                        <tr>
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;border-bottom:1px;">Identification Type:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Identification Number:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Expiration Date:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Country of Issue:</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;border-bottom:1px;">Tax Registration Number:</td>
                        </tr>
                        <tr style="margin-top:5px;">
                            <td style="border: 1px solid rgb(191, 191, 191);border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Identification_Type__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Identification_Number__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;"><apex:outputText value="{0,date,dd-MM-yy}">
        <apex:param value="{!app.ApplicantPersonAccount.Identification_Type_Expiration__pc}" />                                         
    </apex:outputText></td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Identification_Type_Country_of_issue__pc}</td>
                            <td style="border: 1px solid rgb(191, 191, 191);border-left:1px;border-top:1px;Text-align:center;">{!app.ApplicantPersonAccount.Jamaican_Tax_Registration_Number__pc}</td>
                        </tr>
                    </table>
                </div>
            </div>
            <div>
                <h5 style="width:98%;font-size:Small; color:#00a1e0;margin-left:5px;border-bottom: 2px solid #00a1e0;"><b>
                    &nbsp;CONTACT DETAILS
                    </b>
