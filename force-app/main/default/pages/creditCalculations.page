<apex:page
  renderAs="PDF"
  applyBodyTag="false"
  sidebar="false"
  showHeader="false"
  controller="CreditCalculationsController"
>
  <head>
    <style type="text/css" media="print">
      @page {
        @top-center {
          content: element(header);
        }
        @bottom-left {
          content: element(footer);
        }
      }
      div.header {
        padding: 10px;
        position: running(header);
      }
      div.footer {
        display: block;
        padding: 5px;
        margin-bottom: 100px;
        position: running(footer);
      }

      .pagenumber:before {
        content: counter(page);
      }
      .pagecount:before {
        content: counter(pages);
      }
    </style>
  </head>
  <apex:form>
    <div class="footer">
      <div style="text-align: left; font-size: 15px;">
        {!$Label.ApplicationFormText}
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span
          class="pagenumber"
        />
        of <span class="pagecount" />
      </div>
    </div>
    <div class="content">
      <apex:image
        url="{!URLFOR($Resource.creditcalculations)}"
        style="width: 100%; height: 100px;"
      />
    </div>
    <div style="margin-top: 30px;">
      <h3
        style="
          font-size: small;
          width: 60%;
          color: white;
          background-color: #00a1e0;
          font-family: sans-serif;
          height: 25px;
          padding-top: 12px;
          margin-left: 1px;
        "
      >
        &nbsp;SUMMARY OF APPLICANT(S) PERSONAL INFORMATION
      </h3>
      <div style="border: 1px solid #00a1e0; margin: 1px; margin-top: -12px;">
        <apex:variable value="{!0}" var="num" />
        <apex:repeat value="{!assetslst}" var="appitem">
          <apex:variable var="num" value="{!FLOOR(num) + 1}" />
          <h5
            style="
              font-size: small;
              color: black;
              background-color: #92d5f0;
              height: 25px;
              padding-top: 15px;
            "
          >
            <b>
              &nbsp;APPLICANT {!FLOOR(num)}
            </b>
          </h5>
          <div style="margin-top: 5px; font-size: small;">
            <table
              style="margin-left: 16px; margin-right: 10px; width: 95%;"
              cellpadding="0"
              cellspacing="0"
            >
              <tr>
                <td
                  style="
                    text-align: top;
                    border: 1px solid rgb(191, 191, 191);
                    border-right: 5px solid rgb(149, 149, 149);
                    width: 15%;
                    background-color: rgb(242, 242, 242);
                    text-align: center;
                    font-weight: bold;
                  "
                  rowspan="2"
                >
                  GENERAL DETAILS
                </td>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 95%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 40%;">Name of Applicant:</td>
                      <td style="width: 20%;">Age of Applicant:</td>
                      <td style="width: 20%;">Gender:</td>
                      <td style="width: 20%;">Marital Status:</td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 40%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.name}
                      </td>
                      <td
                        style="
                          width: 20%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.FinServ__Age__pc}
                      </td>
                      <td
                        style="
                          width: 20%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.FinServ__Gender__pc}
                      </td>
                      <td
                        style="
                          width: 20%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.Marital_Status__c}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              <tr>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 95%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 35%;">
                        Highest level of education attained:
                      </td>
                      <td style="width: 20%;">Number of Dependents:</td>
                      <td style="width: 25%;">Status of Address:</td>
                      <td style="width: 30%;">
                        Time living at Current Address:
                      </td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 35%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.Highest_Level_of_Education_attained__pc}
                      </td>
                      <td
                        style="
                          width: 20%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.FinServ__NumberOfDependents__pc}
                      </td>
                      <td
                        style="
                          width: 25%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.Status_of_Address__pc}
                      </td>
                      <td
                        style="
                          width: 30%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Account__r.Years_at_Residence__pc}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
          </div>
          <div style="margin-top: 5px; font-size: small;">
            <table
              style="margin-left: 16px; margin-right: 10px; width: 95%;"
              cellpadding="0"
              cellspacing="0"
            >
              <tr>
                <td
                  style="
                    text-align: top;
                    border: 1px solid rgb(191, 191, 191);
                    border-right: 5px solid rgb(149, 149, 149);
                    width: 15%;
                    background-color: rgb(242, 242, 242);
                    text-align: center;
                    font-weight: bold;
                  "
                  rowspan="2"
                >
                  CREDIT ASSESSMENT
                </td>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 80%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 30%;">CRIF Rating:</td>
                      <td style="width: 20%;">CreditInfo Rating:</td>
                      <td style="width: 30%;">Overall Credit Bureau Rating:</td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 30%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.CRIF_Rating__c}
                      </td>
                      <td
                        style="
                          width: 20%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.CreditInfo_Rating__c}
                      </td>
                      <td
                        style="
                          width: 30%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Overall_Credit_Bureau_Rating__c}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              <tr>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 75%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 40%;">
                        Credit History in the last 24 months:
                      </td>
                      <td style="width: 40%;">
                        Assessment of Applicant's Net Worth:
                      </td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 40%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Credit_History_in_last_24_Months__c}
                      </td>
                      <td
                        style="
                          width: 40%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Assessment_of_Applicant_Net_Worth__c}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
          </div>
          <div style="margin-top: 5px; margin-bottom: 10px; font-size: small;">
            <table
              style="margin-left: 16px; margin-right: 10px; width: 95%;"
              cellpadding="0"
              cellspacing="0"
            >
              <tr>
                <td
                  style="
                    text-align: top;
                    border: 1px solid rgb(191, 191, 191);
                    border-right: 5px solid rgb(149, 149, 149);
                    width: 15%;
                    background-color: rgb(242, 242, 242);
                    text-align: center;
                    font-weight: bold;
                  "
                  rowspan="6"
                >
                  CREDIT ASSESSMENT
                </td>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 95%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 20%;">KYC Compliant?</td>
                      <td style="width: 40%;" colspan="4">Time as Member:</td>
                      <td style="width: 15%;">RIM Number:</td>
                      <td style="width: 15%;">Primary Relationship Tier:</td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 20%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Is_applicant_KYC_Compliant__c}
                      </td>
                      <td
                        style="
                          cellspacing: 0px;
                          width: 5%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Time_as_Member_Years__c}
                      </td>
                      <td
                        style="
                          cellspacing: 0px;
                          width: 10%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        years
                      </td>
                      <td
                        style="
                          cellspacing: 0px;
                          width: 5%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Time_as_Member_Months__c}
                      </td>
                      <td
                        style="
                          cellspacing: 0px;
                          width: 10%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        months
                      </td>
                      <td
                        style="
                          width: 15%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Rim_Number__c}
                      </td>
                      <td
                        style="
                          width: 25%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Primary_Relationship_Tier__c}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              <tr>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 75%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 40%;">Affiliation Type:</td>
                      <td style="width: 40%;">Sanction Screening Completed?</td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 40%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Affiliation_Type__c}
                      </td>
                      <td
                        style="
                          width: 40%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.Sanction_Screening_Completed__c}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
              <tr>
                <td style="width: 85%;">
                  <table
                    style="margin-left: 16px; margin-right: 10px; width: 75%;"
                    cellpadding="0"
                    cellspacing="5"
                  >
                    <tr>
                      <td style="width: 30%;">JN Group Employee?</td>
                      <td style="width: 50%;">
                        Will the JN Employee Loan Benefit be applied?
                      </td>
                    </tr>
                    <tr>
                      <td
                        style="
                          width: 30%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.JN_Group_Employee__c}
                      </td>
                      <td
                        style="
                          width: 50%;
                          border: 1px solid rgb(191, 191, 191);
                          text-align: center;
                        "
                      >
                        {!appitem.applicant.JN_Employee_Loan_Benefit__c}
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
          </div>
        </apex:repeat>
      </div>
      <div style="margin-top: 30px;">
        <h3
          style="
            font-size: small;
            width: 60%;
            color: white;
            background-color: #00a1e0;
            font-family: sans-serif;
            height: 25px;
            padding-top: 12px;
            margin-left: 1px;
          "
        >
          &nbsp;PRODUCT DETAILS
        </h3>
        <div style="border: 1px solid #00a1e0; margin: 1px; margin-top: -12px;">
          <apex:pageBlock rendered="{!isAutoFound}">
            <h5
              style="
                font-size: small;
                color: black;
                background-color: #92d5f0;
                height: 25px;
                padding-top: 15px;
              "
            >
              <b>
                &nbsp;PERSONAL AUTO LOAN
              </b>
            </h5>
            <h5
              style="
                width: 98%;
                font-size: Small;
                color: #00a1e0;
                margin-left: 5px;
                border-bottom: 2px solid #00a1e0;
              "
            >
              <b>
                &nbsp;CREDIT DETAILS
              </b>
            </h5>
            <div style="margin-top: 5px; font-size: small;">
              <table
                style="margin-left: 16px; margin-right: 10px; width: 95%;"
                cellpadding="0"
                cellspacing="0"
              >
                <tr>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Loan Purpose:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Vehicle Classification:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Year of Vehicle:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Make and Model of Vehicle:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Repayment Method:
                  </td>
                </tr>
                <tr style="margin-top: 10px;">
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Auto_Loan_Purpose__c==null,'',ProductDetails[0].Auto_Loan_Purpose__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Vehicle_Classification__c==null,'',ProductDetails[0].Vehicle_Classification__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Year_of_Motor_Vehicle__c==null,'',ProductDetails[0].Year_of_Motor_Vehicle__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Make_and_Model_of_Motor_Vehicle__c==null,'',ProductDetails[0].Make_and_Model_of_Motor_Vehicle__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Repayment_Method_Auto__c==null,'',ProductDetails[0].Repayment_Method_Auto__c)}
                  </td>
                </tr>
              </table>
              <table
                style="
                  margin-left: 16px;
                  margin-right: 10px;
                  margin-top: 10px;
                  width: 95%;
                "
                cellpadding="0"
                cellspacing="0"
              >
                <tr>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Market Value of Motor Vehicle:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                  >
                    Purchase Price of Vehicle*
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                    colspan="2"
                  >
                    Processing Fee:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                    colspan="4"
                  >
                    Loan Term:
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      border-bottom: 1px;
                    "
                    colspan="2"
                  >
                    Interest Rate:
                  </td>
                </tr>
                <tr style="margin-top: 10px;">
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    JMD
                    ${!if(ProductDetails[0].Market_Value_of_Vehicle__c==null,'',ProductDetails[0].Market_Value_of_Vehicle__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    JMD
                    ${!if(ProductDetails[0].Purchase_Price_of_Vehicle__c==null,'',ProductDetails[0].Purchase_Price_of_Vehicle__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Indicate_applicable_process_fee_percent__c==null,'',ProductDetails[0].Indicate_applicable_process_fee_percent__c)}%
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    per annum
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Years__c==null,'',ProductDetails[0].Years__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    years
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Months__c==null,'',ProductDetails[0].Months__c)}
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    months
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    {!if(ProductDetails[0].Interest_Rate__c==null,'',ProductDetails[0].Interest_Rate__c)}%
                  </td>
                  <td
                    style="
                      border: 1px solid rgb(191, 191, 191);
                      border-left: 1px;
                      border-top: 1px;
                      text-align: center;
                      height: 15px;
                    "
                  >
                    per annum
                  </td>
                </tr>
              </table>
            </div>
            <div
              style="width: 49%; display: inline-block; vertical-align: top;"
            >
              <h5
                style="
                  width: 98%;
                  font-size: Small;
                  color: #00a1e0;
                  margin-left: 5px;
                  border-bottom: 2px solid #00a1e0;
                "
              >
                <b>
                  &nbsp;TOTAL FINANCING
                </b>
              </h5>
              <table
                style="
                  font-size: Small;
                  margin-right: 10px;
                  width: 100%;
                  margin-top: 7px;
                "
                cellpadding="0"
                cellspacing="0"
              >
                <tr>
                  <td style="text-align: right;">+ Loan Amount (Market)</td>
                  <td
                    style="
                      text-align: center;
                      border: 1px solid rgb(191, 191, 191);
                      height: 15px;
                    "
                  >
                    JMD
                    ${!if(ProductDetails[0].Loan_Amount__c==null,0.00,ProductDetails[0].Loan_Amount__c)}
                  </td>
                </tr>
              </table>

              <apex:pageBlock
                rendered="{!ProductDetails[0].Applicant_2_Jn_Employee__c}"
              >
                <table
                  style="
                    font-size: Small;
                    margin-left: 16px;
                    margin-right: 10px;
                    margin-top: 5px;
