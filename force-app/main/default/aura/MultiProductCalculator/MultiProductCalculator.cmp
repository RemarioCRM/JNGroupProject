<!--   MultiProductCalculator.cmp  -->
<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId" controller="MultiProductCalculatorController">
    <aura:attribute name="isProductDetail" type="boolean" required="false" default="false" access="public"/>
    <aura:attribute name="isRequiredField" type="boolean" required="false" default="true"/>
    <aura:attribute name="isRecordIdM" type="String" required="false" default="false" access="public"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:attribute name="Applicantlist" type="List" />
    <aura:attribute name="Coverageoptions" type="List" />
    <aura:attribute name="selectedValue" type="String"/>
    <aura:attribute name="selectedValueUn" type="String"/>
    <aura:attribute name="IsExistingLoan" type="boolean"/>
    <aura:attribute name="applicant1age" type="boolean" default="false"/>
    <aura:attribute name="applicant2age" type="boolean" default="false"/>
    <aura:attribute name="applicant3age" type="boolean" default="false"/> 
    <aura:attribute name="applicant1ageValue" type="decimal" default="0"/>
    <aura:attribute name="applicant2ageValue" type="decimal" default="0"/>
    <aura:attribute name="applicant3ageValue" type="decimal" default="0"/>
    <aura:attribute name="AutoMarketCurrency" type="String" default=""/>
    <aura:attribute name="MarketValueofVehicleA" type="integer"/>
    <aura:attribute name="RowNum" type="Object[]" default="{}"/>
    <aura:attribute name="applicants" type="String[]"/>
    <aura:attribute name="RDetailAuto" type="Object[]" default="{}"/>
    <aura:attribute name="RDetailUnsecured" type="Object[]" default="{}"/>
    <aura:handler event="c:Requestdetailevent" action="{!c.Requestdetaileventhandler}"/>
    <aura:attribute name="LoanamountMarketAutoloan" type="integer" default="0"/>
    <aura:attribute name="LoanamountJNStaff1Autoloan" type="integer" default="0"/>
    <aura:attribute name="LoanamountJNStaff2Autoloan" type="integer" default="0"/>
    <aura:attribute name="LoanamountJNStaff3Autoloan" type="integer" default="0"/>
    <aura:attribute name="LoanamountMarketAutoloannew" type="integer" default="0"/>
    <aura:attribute name="LoanamountJNStaff1Autoloannew" type="integer" default="0"/>
    <aura:attribute name="LoanamountJNStaff2Autoloannew" type="integer" default="0"/>
    <aura:attribute name="LoanamountJNStaff3Autoloannew" type="integer" default="0"/>
    <aura:attribute name="JNGIAutoloan" type="integer" default="0"/>
    <aura:attribute name="JNLifeAutoloan" type="integer" default="0"/>
    <aura:attribute name="ProcessingFeesAutoloan" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountMarketAutoloan" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff1Autoloan" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff2Autoloan" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff3Autoloan" type="integer" default="0"/>
    <aura:attribute name="BMJNGI1Autoloan" type="integer" default="0"/>
    <aura:attribute name="BMJNGI2Autoloan" type="integer" default="0"/>
    <aura:attribute name="BMJNLifeAutoloan" type="integer" default="0"/>
    <aura:attribute name="BMProcessingFeesAutoloan" type="integer" />
    <aura:attribute name="AMLoanamountMarketAutoloan" type="integer" default="0"/>
    <aura:attribute name="AMLoanamountMarketAutoloannew" type="integer" default="0"/>
    <aura:attribute name="AMLoanamountJNStaff1Autoloan" type="integer" default="0"/>
    <aura:attribute name="AMLoanamountJNStaff2Autoloan" type="integer" default="0"/>
    <aura:attribute name="AMLoanamountJNStaff3Autoloan" type="integer" default="0"/>
    <aura:attribute name="AMJNGI1Autoloan" type="integer" default="0"/>
    <aura:attribute name="AMJNGI2Autoloan" type="integer" default="0"/>
    <aura:attribute name="AMJNLifeAutoloan" type="integer" default="0"/>
    <aura:attribute name="AMProcessingFeesAutoloan" type="integer" default="0"/>
    <aura:attribute name="TotalLoanAmountAutoloan" type="integer" default="0"/>
    <aura:attribute name="LoanTermMarket" type="integer" default="0"/>
    <aura:attribute name="LoanTermMarketUn" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountMarketAutoloanUn" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff1AutoloanUn" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff2AutoloanUn" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff3AutoloanUn" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountMarketAutoloanUnNew" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff1AutoloanUnNew" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff2AutoloanUnNew" type="integer" default="0"/>
    <aura:attribute name="BMLoanamountJNStaff3AutoloanUnNew" type="integer" default="0"/>
    <aura:attribute name="JNLifeCreditorLifePremium1New" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNLifeCreditorLifePremium1New" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNLifeCreditorLifePremium2New" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNGIMotorPremium1stYear1New" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNGIMotorPremium1stYear2New" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNGIMotorPremiumhalfPayment1New" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNGIMotorPremiumhalfPayment2New" type="integer" default="0"/>
    <aura:attribute name="MonthlyProcessingFees1New" type="integer" default="0"/>
    <aura:attribute name="MonthlyProcessingFees2New" type="integer" default="0"/>
    <aura:attribute name="MonthlyLoanPaymentJN12New" type="integer" default="0"/>
    <aura:attribute name="MonthlyLoanPaymentJN22New" type="integer" default="0"/>
    <aura:attribute name="MonthlyLoanPaymentJN32New" type="integer" default="0"/>
    
    <aura:attribute name="LoanAmountMarket1UnNew" type="integer" default="0"/>
    <aura:attribute name="LoanAmountJN11UnNew" type="integer" default="0"/>
    <aura:attribute name="LoanAmountJN21UnNew" type="integer" default="0"/>
    <aura:attribute name="LoanAmountJN31UnNew" type="integer" default="0"/>
    <aura:attribute name="ProcessingFeesincludingGCTUn1New" type="integer" default="0"/>
    <aura:attribute name="MonthlyProcessingFees1UnNew" type="integer" default="0"/>
    <aura:attribute name="MonthlyJNLifeCreditorLifePremium1UnNew" type="integer" default="0"/>
    
    
    
    <aura:attribute name="JNLifeCreditorLifePremium1UnNew" type="integer" default="0"/>
    
    
    <aura:attribute name="disabledA" type="Boolean" default="false"/>
    <aura:attribute name="disabledP" type="Boolean" default="false"/>
    <aura:attribute name="ReqA" type="Boolean" default="false"/>
    <aura:attribute name="ReqP" type="Boolean" default="false"/>
    
    <aura:attribute name="disabledPSA" type="Boolean" default="false"/>
    <aura:attribute name="disabledPSP" type="Boolean" default="false"/>
    <aura:attribute name="ReqPSA" type="Boolean" default="false"/>
    <aura:attribute name="ReqPSP" type="Boolean" default="false"/>
    
    
    
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="sobjectName" type="string"/>
    <aura:attribute name="JNBankCC" type="Boolean" default="false"/>
    <aura:attribute name="JNFundCC" type="Boolean" default="false"/>
    <aura:attribute name="JNBankLOC" type="Boolean" default="false"/>
    <aura:attribute name="JNFundLOC" type="Boolean" default="false"/>
    <aura:attribute name="AutoPromotion" type="String" default=""/>
    <aura:attribute name="UnsecuredPromotion" type="String" default=""/>
    <aura:attribute name="CCPromotion" type="String" default=""/>
    <aura:attribute name="LOCPromotion" type="String" default=""/>
    <aura:attribute name="ApplicantCount" type="integer" default=""/>
    <aura:attribute name="ismoratorium" type="Boolean" default="false"/>
    <aura:attribute name="ExistingInstallmentBefore" type="integer" default="0"/>
    <aura:attribute name="ExistingInstallmentAfter" type="integer" default="0"/>
    <aura:attribute name="ExistingGrossincome" type="integer" default="0"/>
    <aura:handler name="lookupSelect" event="c:sObjectLookupSelectEvent" action="{!c.handleLookupSelectEventProductDetail}" />
    
    <aura:attribute name="isAutoLoanPortionValid" type="Boolean" Default="false"/>
    <aura:attribute name="isAutoPayPortionValid" type="Boolean" Default="false"/>
    <aura:attribute name="isUnsecureLoanPortionValid" type="Boolean" Default="false"/>
    <aura:attribute name="isUnsecurePayPortionValid" type="Boolean" Default="false"/>
    <aura:attribute name="isLocLoanPortionValid" type="Boolean" Default="false"/>
    <aura:attribute name="isLocPayPortionValid" type="Boolean" Default="false"/>
    <aura:attribute name="isProposedUN" type="Boolean" Default="false"/>
    <aura:attribute name="isProposedAuto" type="Boolean" Default="false"/>
	<aura:attribute name="showMultiCalcPdfIcon" type="Boolean" Default="false"/>
    <aura:attribute name="retLoanCalcId" type="String" default=""/>
    
    <aura:attribute name="isAutoPD" type="Boolean" Default="false"/>
    <aura:attribute name="isUnsecurePD" type="Boolean" Default="false"/>
    <aura:attribute name="isCCPD" type="Boolean" Default="false"/>
    <aura:attribute name="isLocPD" type="Boolean" Default="false"/>
    
    
    <aura:attribute name="lblMarketValue" type="string" default="Market Value of Vehicle"/>
    <aura:attribute name="lblMinimum" type="string" default="Minimum (MV,PP)"/>
    
    <aura:attribute name="Locationoptions" type="List" />
    <aura:attribute name="selectedLocation" type="String" default="choose one..."/>
    <aura:attribute
        name="lifeInsuranceCoverageType"
        type="String"
        default="None"/>
    <aura:attribute
        name="insuranceProviderName"
        type="String"
        default="None"/>

    <aura:attribute name="allApplicants" type="Object[]" default="{}"/>
    <aura:attribute name="allApplicationAssetLiabilities" type="Object[]" default="{}"/>
    <aura:attribute name="loanCalculation" type="Object" default="{}"/>
    <aura:attribute name="mainAccountId" type="String" description="account id linked to opportunity for primary applicant"/>
    <aura:attribute name="existingMonthlyPaymentBefore" type="Object" default="{}"/>
    <aura:attribute name="existingMonthlyPaymentAfter" type="Object" default="{}"/>
    <form class="slds-form ">
        <fieldset  class="slds-box slds-theme--default slds-container--large" style="padding-right: 5px;">
            <aura:if isTrue="{!v.isProductDetail==false}">
                <legend class="slds-text-heading--large slds-p-vertical--medium" >Multi Product Calculator</legend>
            </aura:if>
            <aura:if isTrue="{!v.isProductDetail}">
                <legend class="slds-text-heading--large slds-p-vertical--medium" >PRODUCT DETAILS</legend>
            </aura:if>
            
            
            <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_1-of-1" >
                <div  class="slds-section__title slds-border_bottom">Product Selection &amp; Applicant Details</div>
                <div class="slds-grid slds-wrap" style="margin-top:10px;">
                  
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:select label="Select the application product combination" name="selectapplicant"  aura:id="selectapplicant" required="true" onchange="{! c.showhideONmethod }" disabled="{!v.isProductDetail}">
                            <option value="0">choose one...</option>
                            <option value="1">Auto Loan</option>
                            <option value="2">Unsecured Loan</option>
                            <option value="3">Credit Card</option>
                            <option value="4">Line of Credit</option>
                            <option value="5">Auto Loan &amp; Unsecured Loan</option>
                            <option value="6">Auto Loan &amp; Credit Card</option>
                            <option value="7">Auto Loan &amp; Line of Credit</option>
                            <option value="8">Unsecured Loan &amp; Credit Card</option>
                            <option value="9">Unsecured Loan &amp; Line of Credit</option>
                            <option value="10">Credit Card &amp; Line of Credit</option>
                            <option value="11">Auto Loan, Unsecured Loan &amp; Credit Card</option>
                            <option value="12">Auto Loan, Unsecured Loan &amp; Line of Credit</option>
                            <option value="13">Auto Loan, Credit Card &amp; Line of Credit</option>
                            <option value="14">Unsecured Loan, Credit Card &amp; Line of Credit</option>
                            <option value="15">Auto Loan, Unsecured Loan, Credit Card &amp; Line of Credit</option>
                        </lightning:select> 
                    </div>
                    <aura:if isTrue="{!v.isProductDetail==false}">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12" style="padding-right: 5px;padding-left: 5px;">
                            <lightning:select label="Number of Applicant(s)" name="Number of Applicant(s)" aura:id="NumberofApplicant" onchange="{!c.ApplicantSelectionChange}" required="true">
                                <option value="0">One (1) Applicant</option>
                                <option value="1">Two (2) Applicants</option>
                                <option value="2">Three (3) Applicants</option>                                
                            </lightning:select>
                        </div>
                    </aura:if>
                </div>
                
            </div>   
            <aura:if isTrue="{!v.isProductDetail==false}">
                <div class="slds-col slds-size_1-of-1 slds-medium-size_1-of-1 slds-large-size_1-of-1" >    
                    <div class="slds-scrollable_x" style="width:51.9rem;margin-top:10px;">
                        <table class="slds-table slds-table_cell-buffer slds-table_bordered">
                            <thead>
                                <tr class="slds-text-heading--label">
                                    <th scope="col" >
                                        <div class="slds-truncate fs" title="index"></div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate fs" title="Applicant's Last Name">Applicant's<br/> Last Name</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate fs" title="Applicant's First Name">Applicant's<br/> First Name</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate fs" title="Date Of Birth">Date Of Birth*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<br/>(MM/DD/YYYY)</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate fs" title="Gross Monthly Income">Gross Monthly Income*</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate fs" title="Existing Monthly Credit Payment">Existing Monthly Credit<br/> Payment</div>
                                    </th>
                                    <th scope="col">
                                        <div class="slds-truncate fs" title="JN Employee">JN EMPLOYEE*<br/><span style="font-size:6px;">(Check the box if YES)</span></div>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <aura:iteration items="{!v.RowNum}" var="row">
                                    <tr>
                                        <td >
                                            {!row.Id} 
                                        </td>
                                        <td>
                                            <lightning:input type="text"  value="{!row.LastName}"/>
                                        </td>
                                        <td>
                                            <lightning:input type="text"  value="{!row.FirstName}"/>
                                        </td>
                                        <td>
                                            
                                            <lightning:input type="Date" value="{!row.DateOfBirth}" class="pr" messageWhenBadInput="Your entry does not match the allowed format MM/DD/YYYY." style="padding-right: 0px!important;" onblur="{!c.showhideCoverageType}"/>
                                        </td>
                                        <td>
                                            <lightning:input type="number" formatter="currency" step="0.01"  value="{!row.GMIncome}" onblur="{!c.ApplicantDetailsChange}" />
                                        </td>
                                        <td>
                                            <lightning:input type="number"  formatter="currency" step="0.01"  value="{!row.EMCPayment}" onblur="{!c.ApplicantDetailsChange}" />
                                        </td>
                                        <td>
                                            <lightning:input type="checkbox"  checked="{!row.IsJNEmployee}" onchange="{!c.updateRequestdetail}"/>
                                        </td>
                                    </tr>
                                </aura:iteration>
                            </tbody>
                        </table>
                    </div>
                </div>
            </aura:if>
            <div aura:id="AutoLoan">
                <aura:if isTrue="{!v.isProductDetail==false}">
                    <div  class="slds-section__title slds-border_bottom" style="margin-top:10px;">Personal Auto Loan Details</div>
                </aura:if>
                <aura:if isTrue="{!v.isProductDetail}">
                    <div  class="slds-section__title slds-border_bottom" style="margin-top:10px;">Personal Auto Loan Request</div>
                    <div style=" text-decoration: underline;font-style: italic;font-weight: bold;margin-top:10px;">COLLATERAL DETAILS</div>
                </aura:if>
                <div class="slds-grid slds-wrap" style="margin-top:10px;">
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-large-size_4-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:select label="Loan Purpose" name="Loan Purpose"  aura:id="LoanPurpose" required="true" onchange="{! c.showhideONLoanPurpose }">
                            <option value="0">choose one...</option>
                            <option value="1">Purchase a Motor Vehicle</option>
                            <option value="2">Refinance Existing Auto Loan</option>
                            <option value="3">Equity in a Motor Vehicle</option>
                        </lightning:select>
                    </div> 
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_8-of-12 slds-large-size_8-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <aura:if isTrue="{!v.IsExistingLoan == false}">
                            <lightning:select label="$" name="Purchase Price of Vehicle"  aura:id="autoPurchasePriceOfVehicleCurr" class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12 fl">
                                <option value="1">JMD</option>
                                <option value="2">USD</option>
                                <option value="3">CAD</option>
                                <option value="4">GBP</option>
                            </lightning:select>
                            
                            <lightning:input formatter="currency" step="0.01" label="Purchase Price of Vehicle" type="number"  name="Purchase Price of Vehicle" aura:id="PurchasePriceofVehicle" class="slds-col slds-size_1-of-1 slds-medium-size_10-of-12 slds-large-size_10-of-12 fl" onchange="{!c.CalculateMinimumMV}" />
                        </aura:if>
                        <aura:if isTrue="{!v.IsExistingLoan == true}">
                            <lightning:select label="$" name="Loan Balance of Vehicle"  aura:id="LoanBalanceOfVehicleCurr" class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12 fl" >
                                <option value="1">JMD</option>
                                <option value="2">USD</option>
                                <option value="3">CAD</option>
                                <option value="4">GBP</option>
                            </lightning:select>
                            <lightning:input formatter="currency" step="0.01" label="Loan Balance of Vehicle" type="number"  name="Loan Balance of Vehicle" aura:id="LoanBalanceofVehicle" class="slds-col slds-size_1-of-1 slds-medium-size_10-of-12 slds-large-size_10-of-12 fl" onchange="{!c.CalculateMinimumMV}"/> 
                        </aura:if>
                        
                    </div>
                    
                </div>
                <aura:if isTrue="{!v.isProductDetail==false}">
                    <div class="slds-grid slds-wrap">
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-large-size_4-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:select label="Market Currency of Vehicle" name="Market Value of Vehicle"  aura:id="MarketValueofVehicle" required="true" onchange="{! c.UpdateMarketCurrency }">
                            <option value="JMD">JMD</option>
                            <option value="USD">USD</option>
                            <option value="CAD">CAD</option>
                            <option value="GBP">GBP</option>
                        </lightning:select>
                    </div>
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_8-of-12 slds-large-size_8-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:input formatter="currency" step="0.01" label="{!v.lblMarketValue}" type="Number" name="Market Value of Vehicle" aura:id="MarketValueofVehicle1" required="true" onblur="{!c.SetMotorVehicleDeposit}"/>
                    </div>        
                </div>
                </aura:if>
                <aura:if isTrue="{!v.isProductDetail}">
                    <div class="slds-grid slds-wrap">
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-large-size_4-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:select label="Market Currency of Vehicle" name="Market Value of Vehicle"  aura:id="MarketValueofVehicle" required="true" onchange="{! c.UpdateMarketCurrency }">
                            <option value="JMD">JMD</option>
                            <option value="USD">USD</option>
                            <option value="CAD">CAD</option>
                            <option value="GBP">GBP</option>
                        </lightning:select>
                    </div>
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-large-size_4-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:input formatter="currency" step="0.01" label="Market Value of Vehicle" type="Number" name="Market Value of Vehicle" aura:id="MarketValueofVehicle1" required="true" onblur="{!c.SetMotorVehicleDeposit}"/>
                    </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-large-size_4-of-12" style="padding-right: 5px;padding-left: 5px;">
                        <lightning:input formatter="currency" step="0.01" label="{!v.lblMinimum}" type="Number" name="Market Value of Vehicle" aura:id="Minimummv" disabled="true"/>
                    </div>
                </div>
                </aura:if>
                
                <div class="slds-grid slds-wrap"> 
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12">
                        
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12" style="float:left;" >
                            <lightning:input label="%" type="number" disabled="{!v.disabledP}"  name="Motor Vehicle Deposit" aura:id="MotorVehicleDeposit" required="{!v.ReqP}" onblur="{!c.SetMotorVehicleDeposit}"/>
                        </div>
                        <!--<div class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12 mTop" style="float:left;"  >
                            <lightning:input label="" value="%"  aura:id="MotorVehicleDeposit1" disabled="true"/>
                        </div>-->
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12 mTop" style="float:left;" >
                            <lightning:input label=""  value="{!v.AutoMarketCurrency}" aura:id="MotorVehicleDeposit2" disabled="true"/>
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_5-of-12 slds-large-size5-of-12" style="float:left;" >
                            <lightning:input formatter="currency" step="0.01" label="Motor Vehicle Deposit" value="{!v.MarketValueofVehicleA}" aura:id="MotorVehicleDeposit3" disabled="true"/>
                        </div>
                    </div>
                    <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_6-of-12">
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12" style="padding-left: 5px;color: red;float: left;margin-top: 31px;" aura:id="MotorVehicleDepositor" > 
                            OR 
                        </div>
                        <div class="slds-col slds-size_1-of-1 slds-medium-size_2-of-12 slds-large-size_2-of-12" style="float:left"   >
                            <lightning:select label="$" aura:id="MotorVehicleDepositCurr"  disabled="{!v.disabledA}">
                                <option value="1">JMD</option>
                                <option value="2">USD</option>
                                <option value="3">CAD</option>
                                <option value="4">GBP</option>
