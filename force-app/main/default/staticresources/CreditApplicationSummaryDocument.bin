@page {
  margin-top: 3cm;
  @top-center {
    content: element(header);
  }
  @bottom-left {
    content: element(footer);
  }
}
div.header {
  padding: 10px;
  position: running(header);
}
div.footer {
  display: block;
  padding: 5px;
  margin-bottom: 100px;
  position: running(footer);
  font-size: 0.775em;
}
body {
  font-family: "Gill Sans", sans-serif;

  font-size: 0.785em;
}
.childRowFields {
  font-size: 0.785em;
  text-align: left;
  line-height: 20px;
  font-family: "Gill Sans", sans-serif;
  text-align: center;
}
.boxed {
  border: 2px solid rgb(191, 191, 191);
  font-size: 0.785em;
  text-align: center;
  line-height: 30px;
  border-radius: 5px;
  font-family: "Gill Sans", sans-serif;
  margin-right: 10px;
  margin-left: 10px;
}
.applicant1 {
  width: 98%;
  font-size: 0.785em;
  color: #000060;
  margin-left: 5px;
  border-bottom: 2px solid #0068ff;
  font-family: "Gill Sans", sans-serif;
}
.detail {
  font-size: 1em;
  width: 100%;
  color: white;
  background-color: #0068ff;
  font-family: "Gill Sans", sans-serif;
