<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Home_Phone_10_digits</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Home_Number__c)),
    LEN(Home_Number__c) = 10
)</errorConditionFormula>
    <errorMessage>Please review the home number field</errorMessage>
</ValidationRule>
