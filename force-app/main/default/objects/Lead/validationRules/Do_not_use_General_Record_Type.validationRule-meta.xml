<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Do_not_use_General_Record_Type</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISNEW(),
   RecordType.DeveloperName = &quot;General&quot;
)</errorConditionFormula>
    <errorMessage
  >Please use a different record type. Do not use this one</errorMessage>
</ValidationRule>
