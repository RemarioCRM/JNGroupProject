<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Invalid_PreviousEmployerPhone1_Formula</fullName>
    <active>true</active>
    <description>Ensures that the previous employer phone 1 field is properly validated.</description>
    <errorConditionFormula>IF( OR(ISCHANGED(Previous_Employer_Phone_1__c) &amp;&amp; NOT(ISNULL(Previous_Employer_Phone_1__c)) &amp;&amp; NOT(ISBLANK(Previous_Employer_Phone_1__c)) &amp;&amp; !REGEX(Previous_Employer_Phone_1__c ,&#39;^\\d{10,25}$&#39;), ISNEW() &amp;&amp; NOT(ISNULL(Previous_Employer_Phone_1__c)) &amp;&amp; NOT(ISBLANK(Previous_Employer_Phone_1__c)) &amp;&amp; !REGEX(Previous_Employer_Phone_1__c,&#39;^\\d{10,25}$&#39;)),true,false)</errorConditionFormula>
    <errorDisplayField>Previous_Employer_Phone_1__c</errorDisplayField>
    <errorMessage>Invalid format entered for previous employer phone 1. The minimum number of digits should be 10</errorMessage>
</ValidationRule>
